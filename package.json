    "expo": "^44.0.0",
    "expo-cli": "^4.12.6",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.5",
    "firebase": "^9.3.0",
    "jest": "^26.6.3",
    "js-sha256": "^0.9.0",
